<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/PreceptDef.xsd">

    <PreceptDef Abstract="true" Name="PMM_EvMutationPrecept">
        <issue>PMM_EnvironmentalMutation</issue>

    </PreceptDef>

    <PreceptDef ParentName="PMM_EvMutationPrecept">
        <defName>PMM_EvMutationProhibited</defName>
        <label>prohibited</label>
        <description>TODO</description>
        <impact>High</impact>
        <comps>
            <li Class="PreceptComp_UnwillingToDo">
                <eventDef>SowMutagenicPlants</eventDef>
            </li>
            <li Class="PreceptComp_UnwillingToDo">
                <eventDef>CreateMutagenicWeapon</eventDef>
            </li>
        </comps>
    </PreceptDef>

    <PreceptDef ParentName="PMM_EvMutationPrecept">
        <defName>PMM_EvMutationDisliked</defName>
        <label>disliked</label>
        <description>TODO</description>
        <impact>Medium</impact>
        <comps>
            <li Class="PreceptComp_SituationalThought">
                <thought>PMM_UsingMutagenicWeaponBad</thought>
            </li>
            <li Class="PreceptComp_SelfTookMemoryThought">
                <thought>PMM_PlantedMutagenicPlantBad</thought>
                <eventDef>SowMutagenicPlants</eventDef>
            </li>
            <li Class="PreceptComp_SelfTookMemoryThought">
                <thought>PMM_CreatedMutagenicWeaponBad</thought>
                <eventDef>CreateMutagenicWeapon</eventDef>
            </li>
        </comps>
    </PreceptDef>

    <PreceptDef ParentName="PMM_EvMutationPrecept">
        <defName>PMM_EvMutationNeutral</defName>
        <label>neutral</label>
        <description>TODO</description>
        <impact>Low</impact>
        <countsTowardsPreceptLimit>false</countsTowardsPreceptLimit>
    </PreceptDef>

    <PreceptDef ParentName="PMM_EvMutationPrecept">
        <defName>PMM_EvMutationLiked</defName>
        <label>liked</label>
        <description>TODO</description>
        <impact>Low</impact>
        <comps>
            <li Class="PreceptComp_SituationalThought">
                <thought>PMM_UsingMutagenicWeaponGood</thought>
            </li>
        </comps>
    </PreceptDef>

</Defs>