<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/PreceptDef.xsd">
    <PreceptDef Abstract="true" Name="PMM_MutagenicsUsagePreceptBase">
        <issue>PMM_MutagenicsUsage</issue>
    </PreceptDef>

    <PreceptDef ParentName="PMM_MutagenicsUsagePreceptBase">
        <defName>PMM_MutagenicsUnlimited</defName>
        <label>Unlimited</label>
        <countsTowardsPreceptLimit>false</countsTowardsPreceptLimit>
        <impact>Low</impact>
        <conflictingMemes>
            <li>HumanPrimacy</li>
            <li>FleshPurity</li>
            <li>Transhumanist</li>
        </conflictingMemes>
        <associatedMemes>
            <li>NaturePrimacy</li>
            <li>PMM_Mutagenesis</li>
            <li>AnimalPersonhood</li>
        </associatedMemes>
    </PreceptDef>

    <PreceptDef ParentName="PMM_MutagenicsUsagePreceptBase">
        <defName>PMM_MutagenicsGuiltyOnly</defName>
        <label>gulity only</label>
        <countsTowardsPreceptLimit>false</countsTowardsPreceptLimit>
        <impact>Low</impact>
        <associatedMemes>
            <li>NaturePrimacy</li>
            <li>PMM_Mutagenesis</li>
            <li>AnimalPersonhood</li>
        </associatedMemes>
        <comps>
            <li Class="PawnmorpherMutagenesis.PreceptComps.UnwillingTo_Mutate">
                <eventDef>ApplyMutagenicsOn</eventDef>
                <allowGuilty>true</allowGuilty>
            </li>
            <li Class="PawnmorpherMutagenesis.PreceptComps.SelfTookMemoryThought_AppliedMutagen">
                <thought>PMM_GavePawnMutation_Bad</thought>
            </li>
        </comps>
    </PreceptDef>

    <PreceptDef ParentName="PMM_MutagenicsUsagePreceptBase">
        <defName>PMM_Disliked</defName>
        <label>disliked</label>
        <impact>Medium</impact>
        <comps>
            <li Class="PawnmorpherMutagenesis.PreceptComps.SelfTookMemoryThought_AppliedMutagen">
                <thought>PMM_GavePawnMutation_Bad</thought>
            </li>
        </comps>
    </PreceptDef>
    <PreceptDef ParentName="PMM_MutagenicsUsagePreceptBase">
        <defName>PMM_Prohibited</defName>
        <label>prohibited</label>
        <impact>High</impact>
        <comps>
            <li Class="PawnmorpherMutagenesis.PreceptComps.UnwillingTo_Mutate">
                <eventDef>ApplyMutagenicsOn</eventDef>
            </li>
        </comps>
    </PreceptDef>
    
</Defs>