<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Schemas/RitualOutcomeEffectDef.xsd">
    <RitualOutcomeEffectDef Name="TrialOutcomeBase">
        <defName>PMM_ConversionEffect</defName>
        <description>TODO</description>
        <workerClass>PawnmorpherMutagenesis.Rituals.OutcomeEffectWorker_MutagenicConversion</workerClass>
        <startingQuality>0.5</startingQuality>
        <minQuality>0.1</minQuality>
        <maxQuality>0.9</maxQuality>
        <comps>
            <li Class="RitualOutcomeComp_RoomStat">
                <label>room impressiveness</label>
                <statDef>Impressiveness</statDef>
                <curve>
                    <points>
                        <li>0,  0</li>
                        <li>50,  0.1</li>
                        <li>120, 0.2</li>
                    </points>
                </curve>
            </li>
            <li Class="RitualOutcomeComp_ParticipantCount">
                <label>participant count</label>
                <curve>
                    <points>
                        <li>(0,  0.0)</li>
                        <li>(4,  0.2)</li>
                        <li>(10, 0.4)</li>
                        <li>(20, 0.6)</li>
                    </points>
                </curve>
            </li>
            <li Class="RitualOutcomeComp_PawnStatScaled">
                <label>{PAWN_labelShort}'s negotiation ability</label>
                <labelAbstract>Leader's negotiation ability</labelAbstract>
                <statDef>NegotiationAbility</statDef>
                <roleId>converter</roleId>
                <scaledBy>0.25</scaledBy>
            </li>
            <li Class="RitualOutcomeComp_PawnStatScaled">
                <label>{PAWN_labelShort}'s negotiation ability</label>
                <labelAbstract>Defendant's negotiation ability</labelAbstract>
                <statDef>NegotiationAbility</statDef>
                <roleId>convertee</roleId>
                <scaledBy>-0.25</scaledBy>
            </li>

            <!-- comp to give a bonus for morphs/former humans in attendance -->
        </comps>
        <outcomeChances>
            <li>
                <label>Terrible</label>
                <chance>0.05</chance>
                <ideoCertaintyOffset>0.2</ideoCertaintyOffset>
                <description>TODO</description>
                <positivityIndex>-2</positivityIndex>
            </li>
            <li>
                <label>Ineffective</label>
                <chance>0.15</chance>
                <ideoCertaintyOffset>0.12</ideoCertaintyOffset>
                <description>TODO</description>
                <positivityIndex>-1</positivityIndex>
            </li>
            <li> <!-- formatting, {CONVERTEE} converted pawn, {ANIMAL} animal they tf'd into, {MORPH} for the morph the turned into {RITUAL} for the name of the ritual  -->
                <!-- former human -->
                <label>Effective</label>
                <chance>0.6</chance>
                <ideoCertaintyOffset>-0.3</ideoCertaintyOffset>
                <memory>EffectiveConversion</memory> <!-- memory is given to all those in attendance -->
                <description>TODO</description>
                <positivityIndex>1</positivityIndex>
            </li>
            <li>
                <!-- morph  -->
                <label>Masterful</label>
                <chance>0.2</chance>
                <ideoCertaintyOffset>-1</ideoCertaintyOffset>
                <memory>MasterfulConversion</memory>
                <description>TODO</description>
                <positivityIndex>2</positivityIndex>
            </li>
        </outcomeChances>
    </RitualOutcomeEffectDef>
</Defs>